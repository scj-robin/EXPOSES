@Article{CKL14,
AUTHOR = {Cleynen, A. and Koskas, M. and Lebarbier, E. and Rigaill, G. and Robin, S.},
TITLE = {Segmentor3IsBack: an R package for the fast and exact segmentation of Seq-data},
JOURNAL = {Algorithms for Molecular Biology},
VOLUME = {9},
YEAR = {2014},
NUMBER = {1},
PAGES = {6},
URL = {http://www.almob.org/content/9/1/6},
DOI = {10.1186/1748-7188-9-6},
PubMedID = {24612691},
ISSN = {1748-7188},
ABSTRACT = {BACKGROUND:Change point problems arise in many genomic analyses such as the detection of copy number variations or the detection of transcribed regions. The expanding Next Generation Sequencing technologies now allow to locate change points at the nucleotide resolution.RESULTS:Because of its complexity which is almost linear in the sequence length when the maximal number of segments is constant, and as its performance had been acknowledged for microarrays, we propose to use the Pruned Dynamic Programming algorithm for Seq-experiment outputs. This requires the adaptation of the algorithm to the negative binomial distribution with which we model the data. We show that if the dispersion in the signal is known, the PDP algorithm can be used, and we provide an estimator for this dispersion. We describe a compression framework which reduces the time complexity without modifying the accuracy of the segmentation. We propose to estimate the number of segments via a penalized likelihood criterion. We 
illustrate the performance of the proposed methodology on RNA-Seq data.CONCLUSIONS:We illustrate the results of our approach on a real dataset and show its good performance. Our algorithm is available as an R package on the CRAN repository.},
annote = {type:ACL  author:Cleynen author:Koskas author:Lebarbier author:Robin}
}

